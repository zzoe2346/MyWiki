views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder == "2-Area/코딩테스트/solved"
    order:
      - file.name
      - 레이팅
      - 반복수
      - 복습
      - created
      - tags
    sort:
      - property: tags
        direction: ASC
      - property: file.name
        direction: DESC
